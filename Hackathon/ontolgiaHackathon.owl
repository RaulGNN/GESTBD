    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/datasource#Aggregator -->

    <Class rdf:about="http://example.org/datasource#Aggregator">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Un agregador de datos con URI y puerto.</rdfs:comment>
        <rdfs:label>Aggregator</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#Attribute -->

    <Class rdf:about="http://example.org/datasource#Attribute">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#Table"/>
        <rdfs:comment>Un atributo de una tabla con caracteristicas especificas.</rdfs:comment>
        <rdfs:label>Attribute</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#DataSource -->

    <Class rdf:about="http://example.org/datasource#DataSource">
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/datasource#hasFile"/>
                <someValuesFrom rdf:resource="http://example.org/datasource#File"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://webprotege.stanford.edu/RCdzX8QyAeoDv31rDLW4I7f"/>
                <someValuesFrom rdf:resource="http://example.org/datasource#Aggregator"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/datasource#hasDatabase"/>
                <someValuesFrom rdf:resource="http://example.org/datasource#Database"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Fuente de datos principal</rdfs:comment>
        <rdfs:label>Data Source</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#Database -->

    <Class rdf:about="http://example.org/datasource#Database">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Base de datos que contiene tablas estructuradas o no estructuradas.</rdfs:comment>
        <rdfs:label>Database</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#File -->

    <Class rdf:about="http://example.org/datasource#File">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Archivo de datos que tiene un formato especifico y un URI.</rdfs:comment>
        <rdfs:label>File</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#StructuredDataBase -->

    <Class rdf:about="http://example.org/datasource#StructuredDataBase">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#Database"/>
        <rdfs:comment>Datos estructurados organizados en tablas.</rdfs:comment>
        <rdfs:label>Structured Data</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#Table -->

    <Class rdf:about="http://example.org/datasource#Table">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#StructuredDataBase"/>
        <rdfs:comment>Una tabla de datos estructurados.</rdfs:comment>
        <rdfs:label>Table</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#UnstructuredDataBase -->

    <Class rdf:about="http://example.org/datasource#UnstructuredDataBase">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#Database"/>
        <rdfs:comment>Datos no estructurados organizados en indices de Elasticsearch.</rdfs:comment>
        <rdfs:label>Unstructured Data</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#Hive -->

    <Class rdf:about="http://example.org/datasource#Hive">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#Aggregator"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/datasource#aggregates"/>
                <someValuesFrom rdf:resource="http://example.org/datasource#MariaDB"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="http://example.org/datasource#aggregates"/>
                <someValuesFrom rdf:resource="http://example.org/datasource#PostgresSQL"/>
            </Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Hive</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#MariaDB -->

    <Class rdf:about="http://example.org/datasource#MariaDB">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#StructuredDataBase"/>
        <rdfs:label>MariaDB</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#PostgresSQL -->

    <Class rdf:about="http://example.org/datasource#PostgresSQL">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#StructuredDataBase"/>
        <rdfs:label>PostgresSQL</rdfs:label>
    </Class>
    


    <!-- http://example.org/datasource#Elasticsearch -->

    <Class rdf:about="http://example.org/datasource#Elasticsearch">
        <rdfs:subClassOf rdf:resource="http://example.org/datasource#UnstructuredDataBase"/>
        <rdfs:label>Elasticsearch</rdfs:label>
    </Class>
    


