    <DatatypeProperty rdf:about="http://example.org/datasource#hasDescription">
        <rdfs:domain rdf:resource="http://example.org/datasource#Attribute"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#File"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Table"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasDescription</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasFormat -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasFormat">
        <rdfs:domain rdf:resource="http://example.org/datasource#File"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasFormat</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasIndex -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasIndex">
        <rdfs:domain rdf:resource="http://example.org/datasource#Elasticsearch"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasIndex</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasName -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasName">
        <rdfs:domain rdf:resource="http://example.org/datasource#Aggregator"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Attribute"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#DataSource"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Database"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#File"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Table"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasName</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasPort -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasPort">
        <rdfs:domain rdf:resource="http://example.org/datasource#Aggregator"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Database"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasPort</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasType -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasType">
        <rdfs:domain rdf:resource="http://example.org/datasource#Attribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasType</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#hasURI -->

    <DatatypeProperty rdf:about="http://example.org/datasource#hasURI">
        <rdfs:domain rdf:resource="http://example.org/datasource#Aggregator"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#Database"/>
        <rdfs:domain rdf:resource="http://example.org/datasource#File"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasURI</rdfs:label>
    </DatatypeProperty>
    


    <!-- http://example.org/datasource#isKey -->

    <DatatypeProperty rdf:about="http://example.org/datasource#isKey">
