    <!-- http://example.org/datasource#aggregates -->

    <ObjectProperty rdf:about="http://example.org/datasource#aggregates">
        <rdfs:label>aggregates</rdfs:label>
    </ObjectProperty>
    


    <!-- http://example.org/datasource#hasAgregator -->

    <ObjectProperty rdf:about="http://example.org/datasource#hasAgregator">
        <rdfs:label>hasAgregator</rdfs:label>
    </ObjectProperty>
    


    <!-- http://example.org/datasource#hasFile -->

    <ObjectProperty rdf:about="http://example.org/datasource#hasFile">
        <rdfs:label>hasFile</rdfs:label>
    </ObjectProperty>
    

    <!-- http://example.org/datasource#hasDatabase -->

    <ObjectProperty rdf:about="http://example.org/datasource#hasDatabase">
        <rdfs:label>hasDatabase</rdfs:label>
    </ObjectProperty>
    


