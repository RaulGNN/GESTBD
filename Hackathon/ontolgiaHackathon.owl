    <!-- Instancia File - archivo raw -->
    <NamedIndividual rdf:about="http://example.org/datasource#ArchivoFile">
        <rdf:type rdf:resource="http://example.org/datasource#File"/>
        <datasource:hasName>archivo</datasource:hasName>
        <datasource:hasFormat>JSON</datasource:hasFormat>
        <datasource:hasURI>https://github.com/RaulGNN/GESTBD/tree/main/Tareas/data/unstructured</datasource:hasURI>
        <datasource:hasDescription>Archivo con las descripciones de vivienda asociadas al propertyCode sin procesar.</datasource:hasDescription>
    </NamedIndividual>

    <!-- Instancia File - archivo procesado -->
    <NamedIndividual rdf:about="http://example.org/datasource#ArchivoProcesadoFile">
        <rdf:type rdf:resource="http://example.org/datasource#File"/>
        <datasource:hasName>archivo_procesado</datasource:hasName>
        <datasource:hasFormat>JSON</datasource:hasFormat>
        <datasource:hasURI>https://github.com/RaulGNN/GESTBD/tree/main/Tareas/data/unstructured</datasource:hasURI>
        <datasource:hasDescription>Archivo con las descripciones de vivienda asociadas al propertyCode procesadas.</datasource:hasDescription>
    </NamedIndividual>

    <!-- Instancia File - archivo traducido -->
    <NamedIndividual rdf:about="http://example.org/datasource#ArchivoTraducidoFile">
        <rdf:type rdf:resource="http://example.org/datasource#File"/>
        <datasource:hasName>archivo_traducido</datasource:hasName>
        <datasource:hasFormat>JSON</datasource:hasFormat>
        <datasource:hasURI>https://github.com/RaulGNN/GESTBD/tree/main/Tareas/data/unstructured</datasource:hasURI>
        <datasource:hasDescription>Archivo con las descripciones de vivienda asociadas al propertyCode traducidas todas al mismo idioma.</datasource:hasDescription>
    </NamedIndividual>

    <!-- Instancia Postgresql -->
    <NamedIndividual rdf:about="http://example.org/datasource#PostgreSQL_DB">
        <rdf:type rdf:resource="http://example.org/datasource#PostgreSQL"/>
        <datasource:hasName>PostgreSQL</datasource:hasName>
        <datasource:hasURI>postgresql://hive:password@localhost:5432/postgres</datasource:hasURI>
        <datasource:hasPort rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5432</datasource:hasPort>
    </NamedIndividual>

    <!-- Instancia MariaDB -->
    <NamedIndividual rdf:about="http://example.org/datasource#MariaDB_DB">
        <rdf:type rdf:resource="http://example.org/datasource#MariaDB"/>
        <datasource:hasName>MariaDB_GeoDB</datasource:hasName>
        <datasource:hasURI>mariadb+pymysql://root:my_password@localhost:3307/mariaGESDB</datasource:hasURI>
        <datasource:hasPort rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3307</datasource:hasPort>
    </NamedIndividual>

    <!-- ElasticSearch -->
    <NamedIndividual rdf:about="http://example.org/datasource#Elasticsearch_DB">
        <rdf:type rdf:resource="http://example.org/datasource#Elasticsearch"/>
        <datasource:hasName>Elasticsearch</datasource:hasName>
        <datasource:hasURI>http://elasticsearch:9200</datasource:hasURI>
        <datasource:hasPort rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9200</datasource:hasPort>
        <datasource:hasIndex>descripciones_index</datasource:hasIndex>
    </NamedIndividual>

    <!-- Hive -->
    <NamedIndividual rdf:about="http://example.org/datasource#Hive_Aggregator">
        <rdf:type rdf:resource="http://example.org/datasource#Hive"/>
        <datasource:hasName>Hive</datasource:hasName>
        <datasource:hasURI>hive://hiveserver2:10000/estructurados;user=hive</datasource:hasURI>
        <datasource:hasPort rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10000</datasource:hasPort>
        <datasource:aggregates rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
        <datasource:aggregates rdf:resource="http://example.org/datasource#MariaDB_DB"/>
    </NamedIndividual>

    <!-- Tabla Viviendas - Postgresql -->
    <NamedIndividual rdf:about="http://example.org/datasource#Viviendas">
        <rdf:type rdf:resource="http://example.org/datasource#Table"/>
        <datasource:hasName>Viviendas</datasource:hasName>
        <datasource:hasDescription>Tabla que contiene informacion de las viviendas</datasource:hasDescription>
        <datasource:storedIn rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Tabla Viviendas - Atributo PropertyCode -->
    <NamedIndividual rdf:about="http://example.org/datasource#propertyCode">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>propertyCode</datasource:hasName>
        <datasource:hasType>INTEGER</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Identificador unico de la vivienda</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: url (Enlace) -->
    <NamedIndividual rdf:about="http://example.org/datasource#UrlAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>url</datasource:hasName>
        <datasource:hasType>String</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Direccion web especifica donde se encuentra la pagina detallada de la propiedad en Idealista.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: address (Direccion) -->
    <NamedIndividual rdf:about="http://example.org/datasource#AddressAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>address</datasource:hasName>
        <datasource:hasType>String</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Ubicacion fisica de la propiedad, incluyendo calle y numero.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: size (Tamanio) -->
    <NamedIndividual rdf:about="http://example.org/datasource#SizeAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>size</datasource:hasName>
        <datasource:hasType>Float</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Superficie total de la propiedad en metros cuadrados.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: floor (Piso) -->
    <NamedIndividual rdf:about="http://example.org/datasource#FloorAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>floor</datasource:hasName>
        <datasource:hasType>Integer</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</datasource:isNullable>
        <datasource:hasDescription>Nivel del edificio en el que se encuentra la propiedad. Puede estar vacio si la propiedad no esta en un edificio de varios pisos.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: province (Provincia) -->
    <NamedIndividual rdf:about="http://example.org/datasource#ProvinceAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>province</datasource:hasName>
        <datasource:hasType>String</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Provincia en la que esta ubicada la propiedad.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: municipality (Municipio) -->
    <NamedIndividual rdf:about="http://example.org/datasource#MunicipalityAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>municipality</datasource:hasName>
        <datasource:hasType>String</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isNullable>
        <datasource:hasDescription>Municipio o ciudad especifica donde se encuentra la propiedad.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: district (Distrito/Barrio) -->
    <NamedIndividual rdf:about="http://example.org/datasource#DistrictAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>district</datasource:hasName>
        <datasource:hasType>String</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
        <datasource:isNullable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</datasource:isNullable>
        <datasource:hasDescription>Distrito, barrio o zona especifica dentro del municipio donde esta situada la propiedad.</datasource:hasDescription>
        <datasource:storedInDatabase rdf:resource="http://example.org/datasource#PostgreSQL_DB"/>
    </NamedIndividual>

    <!-- Atributo: price (Precio) -->
    <NamedIndividual rdf:about="http://example.org/datasource#PriceAttribute">
        <rdf:type rdf:resource="http://example.org/datasource#Attribute"/>
        <datasource:hasName>price</datasource:hasName>
        <datasource:hasType>Float</datasource:hasType>
        <datasource:isKey rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</datasource:isKey>
